(macro nil! (-> (var_name)
  (qq (= (uq var_name) nil))))

(nil! a)
(p a)


(macro p3 (-> (n)
  (qq (~
    (p (uq n))
    (p (uq n))
    (p (uq n))))))

(p3 10)


(macro flip (-> (a b)
  (qq (~
    (= _c (uq b))
    (= (uq b) (uq a))
    (= (uq a) _c)))))

(= x 1)
(= y 2)
(flip x y)
(p x) ; 2
(p y) ; 1
